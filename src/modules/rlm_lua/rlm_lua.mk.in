TARGETNAME	:= @targetname@

ifneq "$(TARGETNAME)" ""
TARGET		:= $(TARGETNAME).a
endif

SOURCES		:= $(TARGETNAME).c lua.c

SRC_CFLAGS	:= @lua_cflags@
TGT_LDLIBS	:= @lua_ldflags@

TGT_PREREQS	:= libfreeradius-lua.a

install: $(R)$(modconfdir)/lua/example.lua

$(R)$(modconfdir)/%: raddb/mods-config/mods-config/%
	@$(ECHO) INSTALL $(notdir $<)
	@$(INSTALL) -m 755 $< $(@D)
