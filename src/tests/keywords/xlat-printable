#
#  PRE: if
#
string printable
string unprintable

printable = "游꼴abcdef游꼴"
unprintable = "\t游꼴\n"

# Relaxed mode allows tabs and spaces
if (!%str.printable("\t 游꼴", "yes")) {
	test_fail
}

if (%str.printable("\xfe")) {
	test_fail
}

if (%str.printable(%{unprintable})) {
	test_fail
}

if (%str.printable("\000abcd")) {
	test_fail
}

if (!%str.printable(%{printable})) {
	test_fail
}

success
