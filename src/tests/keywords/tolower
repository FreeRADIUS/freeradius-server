#
# PRE: if update
#
&request.Tmp-String-0 := "AbCdE"
&request.Tmp-String-0 := "%{tolower:%{request.Tmp-String-0}}"
&request.Tmp-String-1 := "%{tolower:AAbbCCdd}"
&request.Tmp-String-2 := "%{tolower:%{request.Tmp-String-9}}"

if (&request.Tmp-String-0 != "abcde") {
	test_fail
}

if (&request.Tmp-String-1 != "aabbccdd") {
	test_fail
}

if (&request.Tmp-String-2 != "") {
	test_fail
}

success
