#
# PRE: update if
#

&request.Tmp-String-0 := "%(config:modules.test.boolean)"

if (&request.Tmp-String-0 != "no") {
        test_fail
}

&request.Tmp-String-0 := "test"
&request.Tmp-Integer-0 := "%(config:modules.%{request.Tmp-String-0}.integer)"

if (&request.Tmp-Integer-0 != 1) {
        test_fail
}

success