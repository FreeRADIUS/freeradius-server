
&request.Tmp-String-0 := "%{rand:-1}"

#
#  Negative limit should have failed
#
if (&request.Tmp-String-0 != '') {
	test_fail
}

&request.Tmp-String-0 := "%{rand:hello world}"

#
#  Invalid limit should have failed
#
if (&request.Tmp-String-0 != '') {
	test_fail
}

&request.Tmp-Integer-0 := "%{rand:123}"

#
#  Make sure random number is whithin limit
#
if (&request.Tmp-Integer-0 < 0 || &request.Tmp-Integer-0 > 123) {
	test_fail
}

success
