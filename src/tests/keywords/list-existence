#
#  List existence checks are "is the list empty" checks.
#
if (&session-state.) {
	test_fail
}

if (&session-state.[*]) {
	test_fail
}

if (&session-state.) {
	test_fail
}

update session-state {
	&User-Name := 'foo'
}

if (!&session-state.) {
	test_fail
}

if (!&session-state.[*]) {
	test_fail
}

success
