#
#  PRE: update
#
update control {
       Cleartext-Password := 'hello'
}

update request {
       Called-Station-Id  := "This is a test"
       Calling-Station-Id := "This is a test"
}

#
#  Check attribute references
#
if (Called-Station-Id == &Calling-Station-Id) {
	update reply {
		Filter-Id := "filter"
	}
}
