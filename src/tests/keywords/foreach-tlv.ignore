#
#  PRE: foreach
#
#  Loop over children of a TLV.
#
#  Note that the only data types compatible with all data types are "string" or "octets"
#
string out

&Tmp-TLV-0 := {
	&a = 1
	&b = 2
	&c = "foo"
	&d = 127.0.0.1
}

&request += {
	&Tmp-TLV-0 = {
		&a = 2112
		&b = 6809
		&c = "bar"
		&d = 192.0.2.1
	}
}


&out = ""

#
#  Home-brew concat!
#
foreach child (&Tmp-TLV-0[*]) {
	&out += &child.c
	&out += " "
}

&out -= " "

"%{out}"
