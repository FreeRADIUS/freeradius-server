#
# PRE: if update
#
&request.Tmp-String-0 := "AbCdE"
&request.Tmp-String-0 := "%{toupper:%{Tmp-String-0}}"
&request.Tmp-String-1 := "%{toupper:AAbbCCdd}"
&request.Tmp-String-2 := "%{toupper:%{Tmp-String-9}}"

if (&request.Tmp-String-0 != "ABCDE") {
	test_fail
}

if (&request.Tmp-String-1 != "AABBCCDD") {
	test_fail
}

if (&request.Tmp-String-2 != "") {
	test_fail
}

success
