#
#  PRE: foreach foreach-break
#

&request.Tmp-String-0 := "ABCDEF_8"

&control.Tmp-String-0 := "0"
&control.Tmp-String-0 += "1"
&control.Tmp-String-0 += "2"
&control.Tmp-String-0 += "3"
&control.Tmp-String-0 += "4"
&control.Tmp-String-0 += "5"
&control.Tmp-String-0 += "6"
&control.Tmp-String-0 += "7"
&control.Tmp-String-0 += "8"
&control.Tmp-String-0 += "9"
&control.Tmp-String-0 += "a"
&control.Tmp-String-0 += "b"
&control.Tmp-String-0 += "c"
&control.Tmp-String-0 += "d"
&control.Tmp-String-0 += "e"
&control.Tmp-String-0 += "f"
&control.Tmp-String-0 += "g"

foreach &control.Tmp-String-0 {
	if ("%{Tmp-String-0[*]}" =~ /([A-Z0-9\-]*)_%{Foreach-Variable-0}/) {
		&request.Tmp-String-0 := "%{1}"

		success
		break
	}
	elsif ("%{Foreach-Variable-0}" == '9') {
		test_fail
		reject
	}
}
