&request.Calling-Station-Id := "8"

&control.Calling-Station-Id := "0"
&control.Calling-Station-Id += "1"
&control.Calling-Station-Id += "2"
&control.Calling-Station-Id += "3"
&control.Calling-Station-Id += "4"
&control.Calling-Station-Id += "5"
&control.Calling-Station-Id += "6"
&control.Calling-Station-Id += "7"
&control.Calling-Station-Id += "8"
&control.Calling-Station-Id += "9"
&control.Calling-Station-Id += "a"
&control.Calling-Station-Id += "b"
&control.Calling-Station-Id += "c"
&control.Calling-Station-Id += "d"
&control.Calling-Station-Id += "e"
&control.Calling-Station-Id += "f"
&control.Calling-Station-Id += "g"

foreach &control.Calling-Station-Id {
	if (&request.Calling-Station-Id == "%{Foreach-Variable-0}") {
		success
		break
	}
	elsif ("%{Foreach-Variable-0}" == '9') {
		test_fail

		reject
	}
}
