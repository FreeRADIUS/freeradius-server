#
# PRE:
#
&request.Tmp-String-0		:= '9870'
&request.Tmp-Octets-0		:= 0x39383731
&request.Tmp-IP-Address-0	:= 57.56.55.50
&request.Tmp-Integer-0		:= 959985460
&request.Tmp-Cast-IfId		:= '0000:0000:3938:3737'
&request.Tmp-Cast-IPv6Addr	:= '::3938:3738'
&request.Tmp-Cast-IPv6Prefix	:= '::3938:3739/128'
&request.Tmp-Cast-Byte		:= 58
&request.Tmp-Cast-Short		:= 14139
&request.Tmp-Cast-Ether		:= 00:00:39:38:37:3c
&request.Tmp-Cast-Integer64 	:= 1152921505566832445
&request.Tmp-Cast-IPv4Prefix	:= 57.56.55.62/32

&request.Tmp-String-0		:= "%{hex:%{Tmp-String-0}}"
&request.Tmp-String-1		:= "%{hex:%{Tmp-Octets-0}}"
&request.Tmp-String-2		:= "%{hex:%{Tmp-IP-Address-0}}"
&request.Tmp-String-4		:= "%{hex:%{Tmp-Integer-0}}"
&request.Tmp-String-6		:= "%{hex:%{Tmp-Cast-Ifid}}"
&request.Tmp-String-7		:= "%{hex:%{Tmp-Cast-IPv6Addr}}"
&request.Tmp-String-8		:= "%{hex:%{Tmp-Cast-IPv6Prefix}}"
&request.Tmp-String-9		:= "%{hex:%{Tmp-Cast-Byte}}"

# String
if (&request.Tmp-String-0 != '39383730') {
	test_fail
}

# Octets
if (&request.Tmp-String-1 != '39383731') {
	test_fail
}

# IP Address
if (&request.Tmp-String-2 != '39383732') {
	test_fail
}

# Integer
if (&request.Tmp-String-4 != '39383734') {
	test_fail
}

# ifid
if (&request.Tmp-String-6 != '0000000039383737') {
	test_fail
}

# ipv6addr
if (&request.Tmp-String-7 != '00000000000000000000000039383738') {
	test_fail
}

# ipv6addrprefix
if (&request.Tmp-String-8 != '008000000000000000000000000039383739') {
	test_fail
}

# byte
if (&request.Tmp-String-9 != '3a') {
	test_fail
}

&request.Tmp-String-0        := "%{hex:%{Tmp-Cast-Short}}"
&request.Tmp-String-1        := "%{hex:%{Tmp-Cast-Ether}}"
&request.Tmp-String-2        := "%{hex:%{Tmp-Cast-Integer64}}"
&request.Tmp-String-3        := "%{hex:%{Tmp-Cast-IPv4Prefix}}"
&request.Tmp-String-4        := "%{hex:%{Tmp-Octets-9}}"

# short
if (&request.Tmp-String-0 != '373b') {
	test_fail
}

# ethernet
if (&request.Tmp-String-1 != '00003938373c') {
	test_fail
}

# integer64
if (&request.Tmp-String-2 != '100000003938373d') {
	test_fail
}

# ipv4prefix
if (&request.Tmp-String-3 != '203938373e') {
	test_fail
}

# Empty input
if (&request.Tmp-String-4 != '') {
	test_fail
}

success
