#
#  PRE: wimax
#
Vendor-Specific.FreeRADIUS.Proxied-To  := 127.0.0.2

#
#  Check that a known but malformed attr is malformed
#
raw.26.24757.84.9.5.7 := 0xab

if (!(raw.26.24757.84.9.5.7 == 0xab)) {
	test_fail
}

#
#  Check that an unknown attr is OK
#
raw.26.24757.84.9.5.15 := 0xabcdef

if (!(raw.26.24757.84.9.5.15 == 0xabcdef)) {
	test_fail
}

#
#  Check that unknown attributes which are defined
#  get automatically resolved to the real attribute.
#
if (raw.26.11344.1 == 127.0.0.1) {
	test_fail
}

if (!(raw.26.11344.1 == 127.0.0.2)) {
	test_fail
}

raw.26.11344.1 := 0x7f000001

if (Vendor-Specific.FreeRADIUS.Proxied-To == 127.0.0.2) {
	test_fail
}

if (!(Vendor-Specific.FreeRADIUS.Proxied-To == 127.0.0.1)) {
	test_fail
}

if (raw.26.11344.1 == 127.0.0.2) {
	test_fail
}

if (!(raw.26.11344.1 == 127.0.0.1)) {
	test_fail
}

raw.26.66.1 = 0x01020304

if (!(raw.26.66.1 == 0x01020304)) {
	test_fail
}

success
