#
#  PRE: edit
#
update request {
	&Tmp-Integer-0 := 4
	&Tmp-Integer-1 := 5
}

#
#  First modification succeeds.  The second modification fails, which
#  also reverts the changes to the first one.
#
&Tmp-Integer-0 += 5
&Tmp-Integer-1 -= 16	# Run-time underflow!

#
#  The various edits above should have been reverted.
#
if (&Tmp-Integer-0 != 4) {
	test_fail
}

success
