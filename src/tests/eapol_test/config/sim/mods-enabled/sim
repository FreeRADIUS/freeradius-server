cache eap_sim_cache {
	#
	#  key:: Cache key.
	#
	key = "%{eap-aka-sim.Session-ID}"

	#
	#  ttl:: TTL for cache entries.
	#
	ttl = 15

	#
	#  update <section> { ... }::
	#
	update {
		session-state.Session-Data		:= session-state.Session-Data
		session-state.Encr-Data.Counter	:= session-state.Encr-Data.Counter
	}
}
